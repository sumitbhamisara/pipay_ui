<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Merchant Approval</h1>
    <% merchants.forEach((merchant)=>{ %>
        username: <%= merchant.username %>
        <br>
        status: <%= merchant.status %>
        <br>
        <a href="/admin/api/approve/<%=  merchant.username %>">
            <button style="margin-top: 5px;">Approve</button>
        </a>
        <br>
        <a href="/admin/api/reject/<%=  merchant.username %>">
            <button style="margin-top: 5px;">Reject</button>
        </a>
        <br>
        <br>
        <br>
    <% }) %>
</body>
</html>